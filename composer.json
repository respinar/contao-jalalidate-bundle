    "autoload": {
        "psr-4": {
            "Respinar\\ContaoJalalidateBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Respinar\\ContaoJalalidateBundle\\ContaoManager\\Plugin"
    },
    "version": "0.1.20"
